{
  "name": "batchql",
  "version": "0.0.7",
  "description": "Don't just graphql, batchql.",
  "scripts": {
    "prestart": "rimraf build/*",
    "start": "tsc -w src/index.ts --outDir build -d",
    "prebuild": "rimraf build",
    "build": "cross-env NODE_ENV=production tsc src/index.ts --outDir build -d; echo 'done.';",
    "commit": "git add . && git commit -am \"comitting and pushing new patch version\"",
    "push": "git push origin --all",
    "deploy": "npm-run-all build commit n push",
    "n": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/matthiasak/clan"
  },
  "keywords": [
    "graphql",
    "mux",
    "multiplex"
  ],
  "main": "build/index.js",
  "author": "Matt Keas (@matthiasak)",
  "license": "MIT",
  "dependencies": {
    "clan-fp": "^0.0.50"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "lodash.debounce": "^4.0.8",
    "npm-run-all": "^4.0.0",
    "rimraf": "^2.5.4",
    "typescript": "^2.1.4"
  }
}
